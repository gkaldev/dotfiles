set cncpcompletion
set noautofocus
let blacklists = ["https://mail.google.com/*", "https://docs.google.com", "https://inbox.google.com"]
let completionengines = ["google", "wikipedia", "amazon", "youtube"]
let searchengine ghub = "https://github.com/search?utf8=âœ“&q=%s"
let searchalias g = "google"
map gb :buffer<Space>
map <C-i> rootFrame
map <A-)> previousTab
map <A-(> nextTab
map J nextTab
map K previousTab
map <C-d> scrollPageDown
map <C-e> scrollPageUp
map <C-u> scrollPageUp
set autoupdategist
let mapleader = ","
let hintcharacters = "asdfgqwertcvbhjklnmyuiop"

"can type chars to filter link hints
set typelinkhints
"switch between numeric and alphanum
map <C-i> :set numerichints!<CR>

" alias ':g' to ':tabnew google'
command g tabnew google
